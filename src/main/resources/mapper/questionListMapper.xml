<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whychapedia.mapper.QuestionListMapper">

	<!--전체 질문게시판 가져오기  -->
	<select id="selectQuestionListAll" resultType="com.whychapedia.vo.QuestionListVo">
		select * from question_list where user_id=#{sessionId}
	</select>
	<!--questionId에 해당하는 질문내용 가져오기  -->
	<select id="selectQuestionOne" resultType="com.whychapedia.vo.QuestionListVo">
		select * from question_list where id=#{questionId}
	</select>
	<!--새 질문 등록하기  -->
	<insert id="insertQuestionList">
		insert into question_list values (11,#{question_title},#{question_content},0,6,sysdate,1)
	</insert>
</mapper>